<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane alignment="CENTER" prefHeight="400" prefWidth="600" style="-fx-background-color: rgba(0, 0, 0, 0.5);;"
           xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" spacing="20">
        <Button fx:id="PlayContinue" onAction="#onPlayContinue" prefHeight="80" prefWidth="185" text="Play Continue" />
        <Button fx:id="ReturnMenu" onAction="#onReturnMenu" prefHeight="80" prefWidth="185" text="Return to Menu" />
    </VBox>
</StackPane>